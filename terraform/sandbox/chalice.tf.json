{
  "resource": {
    "aws_iam_role": {
      "api_handler_role": {
        "name": "gularte_cabin_shared_calendar-sandbox-api_handler",
        "assume_role_policy": "{\"Version\": \"2012-10-17\", \"Statement\": [{\"Sid\": \"\", \"Effect\": \"Allow\", \"Principal\": {\"Service\": \"lambda.amazonaws.com\"}, \"Action\": \"sts:AssumeRole\"}]}"
      }
    },
    "aws_iam_role_policy": {
      "api_handler_role": {
        "name": "api_handler_rolePolicy",
        "policy": "{\"Version\": \"2012-10-17\", \"Statement\": [{\"Sid\": \"Editor\", \"Effect\": \"Allow\", \"Action\": [\"dynamodb:GetItem\", \"dynamodb:BatchGetItem\", \"dynamodb:BatchWriteItem\", \"dynamodb:DeleteItem\", \"dynamodb:PutItem\", \"dynamodb:Query\", \"dynamodb:Scan\", \"dynamodb:UpdateItem\", \"xray:*\"], \"Resource\": \"*\"}]}",
        "role": "${aws_iam_role.api_handler_role.id}"
      }
    },
    "aws_lambda_function": {
      "api_handler": {
        "function_name": "gularte_cabin_shared_calendar-sandbox",
        "runtime": "python3.8",
        "handler": "app.app",
        "memory_size": 128,
        "tags": {
          "application": "gularte-shared-cabin-calendar",
          "development_type": "backend",
          "environment": "sandbox",
          "criticality": "low",
          "aws-chalice": "version=1.20.0:stage=sandbox:app=gularte_cabin_shared_calendar"
        },
        "timeout": 60,
        "source_code_hash": "${filebase64sha256(\"${path.module}/deployment.zip\")}",
        "filename": "${path.module}/deployment.zip",
        "environment": {
          "variables": {
            "RUN_ENV": "sandbox"
          }
        },
        "role": "${aws_iam_role.api_handler_role.arn}"
      }
    },
    "aws_api_gateway_rest_api": {
      "rest_api": {
        "body": "${data.template_file.chalice_api_swagger.rendered}",
        "name": "gularte-cabin-calendar-backend",
        "binary_media_types": [
          "application/octet-stream",
          "application/x-tar",
          "application/zip",
          "audio/basic",
          "audio/ogg",
          "audio/mp4",
          "audio/mpeg",
          "audio/wav",
          "audio/webm",
          "image/png",
          "image/jpg",
          "image/jpeg",
          "image/gif",
          "video/ogg",
          "video/mpeg",
          "video/webm"
        ],
        "endpoint_configuration": {
          "types": [
            "REGIONAL"
          ]
        }
      }
    },
    "aws_api_gateway_deployment": {
      "rest_api": {
        "stage_name": "sandbox",
        "stage_description": "${md5(data.template_file.chalice_api_swagger.rendered)}",
        "rest_api_id": "${aws_api_gateway_rest_api.rest_api.id}",
        "lifecycle": {
          "create_before_destroy": true
        }
      }
    },
    "aws_lambda_permission": {
      "rest_api_invoke": {
        "function_name": "gularte_cabin_shared_calendar-sandbox",
        "action": "lambda:InvokeFunction",
        "principal": "apigateway.amazonaws.com",
        "source_arn": "${aws_api_gateway_rest_api.rest_api.execution_arn}/*"
      }
    }
  },
  "terraform": {
    "required_version": "> 0.11.0, < 0.13.0"
  },
  "provider": {
    "template": {
      "version": "~> 2"
    },
    "aws": {
      "version": "~> 2"
    },
    "null": {
      "version": "~> 2"
    }
  },
  "data": {
    "aws_caller_identity": {
      "chalice": {}
    },
    "aws_partition": {
      "chalice": {}
    },
    "aws_region": {
      "chalice": {}
    },
    "null_data_source": {
      "chalice": {
        "inputs": {
          "app": "gularte_cabin_shared_calendar",
          "stage": "sandbox"
        }
      }
    },
    "template_file": {
      "chalice_api_swagger": {
        "template": "{\"swagger\": \"2.0\", \"info\": {\"version\": \"1.0\", \"title\": \"gularte-cabin-calendar-backend\"}, \"schemes\": [\"https\"], \"paths\": {\"/healthcheck\": {\"get\": {\"consumes\": [\"application/json\"], \"produces\": [\"application/json\"], \"responses\": {\"200\": {\"description\": \"200 response\", \"schema\": {\"$ref\": \"#/definitions/Empty\"}}}, \"x-amazon-apigateway-integration\": {\"responses\": {\"default\": {\"statusCode\": \"200\"}}, \"uri\": \"${aws_lambda_function.api_handler.invoke_arn}\", \"passthroughBehavior\": \"when_no_match\", \"httpMethod\": \"POST\", \"contentHandling\": \"CONVERT_TO_TEXT\", \"type\": \"aws_proxy\"}}}}, \"definitions\": {\"Empty\": {\"type\": \"object\", \"title\": \"Empty Schema\"}}, \"x-amazon-apigateway-binary-media-types\": [\"application/octet-stream\", \"application/x-tar\", \"application/zip\", \"audio/basic\", \"audio/ogg\", \"audio/mp4\", \"audio/mpeg\", \"audio/wav\", \"audio/webm\", \"image/png\", \"image/jpg\", \"image/jpeg\", \"image/gif\", \"video/ogg\", \"video/mpeg\", \"video/webm\"]}"
      }
    }
  },
  "output": {
    "EndpointURL": {
      "value": "${aws_api_gateway_deployment.rest_api.invoke_url}"
    },
    "RestAPIId": {
      "value": "${aws_api_gateway_rest_api.rest_api.id}"
    }
  }
}
